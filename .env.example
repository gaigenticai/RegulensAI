# =============================================================================
# ENHANCED SECURITY MODULE CONFIGURATION
# =============================================================================

# Web Application Firewall (WAF) Settings
WAF_ENABLED=true
# Enable/disable the Web Application Firewall protection
WAF_BLOCK_MODE=true
# Set to true to block malicious requests, false to log only
WAF_OWASP_CORE_RULES_ENABLED=true
# Enable OWASP Core Rule Set for comprehensive protection
WAF_SQL_INJECTION_PROTECTION=true
# Enable SQL injection attack detection and blocking
WAF_XSS_PROTECTION=true
# Enable Cross-Site Scripting (XSS) attack protection
WAF_CUSTOM_RULES_PATH=/etc/regulateai/waf/custom_rules.conf
# Path to custom WAF rules configuration file

# Advanced Rate Limiting Configuration
RATE_LIMIT_ENHANCED_ENABLED=true
# Enable enhanced rate limiting with token bucket algorithm
RATE_LIMIT_DEFAULT_RPM=1000
# Default requests per minute per IP address
RATE_LIMIT_BURST_SIZE=50
# Maximum burst size for token bucket algorithm
RATE_LIMIT_REDIS_URL=redis://localhost:6379/1
# Redis instance for distributed rate limiting state
RATE_LIMIT_CLEANUP_INTERVAL_SECONDS=300
# Interval for cleaning up expired rate limit entries

# IP Filtering and Geolocation
IP_FILTER_ENABLED=true
# Enable/disable IP address filtering
IP_FILTER_WHITELIST_ENABLED=false
# Set to true to use whitelist mode (only allow listed IPs)
IP_FILTER_GEOLOCATION_ENABLED=true
# Enable geolocation-based IP filtering
IP_FILTER_BLOCKED_COUNTRIES=CN,RU,KP,IR
# Comma-separated list of blocked country codes
IP_FILTER_DATABASE_PATH=/var/lib/regulateai/geoip/GeoLite2-Country.mmdb
# Path to MaxMind GeoIP database file

# Advanced Threat Detection
THREAT_DETECTION_ENABLED=true
# Enable/disable advanced threat detection
THREAT_DETECTION_ML_MODEL_PATH=/var/lib/regulateai/models/threat_detection.onnx
# Path to machine learning model for threat detection
THREAT_DETECTION_THRESHOLD=0.7
# Threat score threshold (0.0-1.0) for blocking requests
THREAT_DETECTION_BEHAVIORAL_ANALYSIS=true
# Enable behavioral analysis for anomaly detection

# Enhanced Security Headers
SECURITY_HEADERS_ENABLED=true
# Enable/disable automatic security headers
SECURITY_HEADERS_HSTS_MAX_AGE=31536000
# HTTP Strict Transport Security max age in seconds
SECURITY_HEADERS_CSP_POLICY=default-src 'self'; script-src 'self' 'unsafe-inline'
# Content Security Policy directive

# =============================================================================
# MULTI-LEVEL CACHING MODULE CONFIGURATION
# =============================================================================

# Multi-Level Cache Settings
CACHE_ENABLED=true
# Enable/disable the multi-level caching system
CACHE_L1_ENABLED=true
# Enable L1 (in-memory) cache layer
CACHE_L2_ENABLED=true
# Enable L2 (Redis) cache layer
CACHE_L3_ENABLED=false
# Enable L3 (database) cache layer

# L1 Memory Cache Configuration
CACHE_L1_MAX_CAPACITY=10000
# Maximum number of entries in L1 cache
CACHE_L1_MAX_SIZE_MB=100
# Maximum memory size for L1 cache in megabytes
CACHE_L1_TTL_MINUTES=60
# Default time-to-live for L1 cache entries in minutes
CACHE_L1_IDLE_TIMEOUT_MINUTES=30
# Time-to-idle timeout for L1 cache entries

# L2 Redis Cache Configuration
CACHE_L2_REDIS_URL=redis://localhost:6379/2
# Redis instance for L2 cache (different DB from rate limiting)
CACHE_L2_MAX_CONNECTIONS=20
# Maximum Redis connections for L2 cache
CACHE_L2_CONNECTION_TIMEOUT_SECONDS=5
# Redis connection timeout in seconds
CACHE_L2_KEY_PREFIX=regulateai:cache:
# Prefix for all L2 cache keys
CACHE_L2_COMPRESSION_ENABLED=true
# Enable compression for L2 cache entries

# L3 Database Cache Configuration
CACHE_L3_DATABASE_URL=postgresql://regulateai:password@localhost:5432/regulateai_cache
# Database URL for L3 cache storage
CACHE_L3_TABLE_NAME=cache_entries
# Table name for L3 cache entries
CACHE_L3_MAX_CONNECTIONS=5
# Maximum database connections for L3 cache
CACHE_L3_CLEANUP_INTERVAL_HOURS=1
# Interval for cleaning up expired L3 cache entries
CACHE_L3_MAX_ENTRY_SIZE_MB=1
# Maximum size per L3 cache entry in megabytes

# Cache Compression and Serialization
CACHE_COMPRESSION_ALGORITHM=lz4
# Compression algorithm: none, lz4, zstd, gzip
CACHE_COMPRESSION_LEVEL=1
# Compression level (1-9 for gzip, 1-22 for zstd, 1-12 for lz4)
CACHE_COMPRESSION_THRESHOLD_BYTES=1024
# Minimum size in bytes before compression is applied
CACHE_SERIALIZATION_FORMAT=bincode
# Serialization format: json, bincode, messagepack
CACHE_MAX_ENTRY_SIZE_MB=10
# Maximum size per cache entry in megabytes

